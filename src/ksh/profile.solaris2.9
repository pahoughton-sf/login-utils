if [ -z "$ssh_env" ] ; then
  PATH=/usr/dt/bin:/usr/openwin/bin:$PATH
  [ -d /usr/local/bin ] && PATH=/usr/local/bin:$PATH
  [ -d /Tools/bin ] && PATH=/Tools/bin:$PATH
  PATH=.:$PATH

  [ -d /usr/local/lib ] && LD_LIBRARY_PATH=/usr/local/lib:$LD_LIBRARY_PATH
  [ -d /Tools/lib ] && LD_LIBRARY_PATH=/usr/local/lib:$LD_LIBRARY_PATH
fi

case $- in
  *i*)
    . .kshrc
    ;;
esac

