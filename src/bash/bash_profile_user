# Note, can also be set in bash_profile_HOSTNAME and bash_profile_OSNAME
host_prepend_path=""


export REPLYTO=@email_addr@

[ -f $HOME/.bash_profile_user_$HOSTNAME ] &&
  . $HOME/.bash_profile_user_$HOSTNAME

export CVS_RSH=ssh
export CVSROOT=@cvs_root@


